{
  "files.associations": {
    "map": "cpp",
    "cmath": "cpp",
    "*.tcc": "cpp",
    "deque": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "unordered_set": "cpp",
    "vector": "cpp",
    "any": "cpp",
    "system_error": "cpp",
    "iostream": "cpp"
  },
  "window.zoomLevel": 0,
  "editor.lineHeight": 28,
  "editor.formatOnSave": true,
  "editor.fontSize": 16,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "editor.tabSize": 6,
  "emmet.includeLanguages": { "javascript": "javascriptreact" },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
